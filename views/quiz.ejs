<!DOCTYPE html>
<html>
<head>
    <title>Create Quiz</title>
    <link rel="stylesheet" type="text/css" href="./css/quiz.css">
    <script src="./js/quiz.js"></script>
    <script src="https://www.gstatic.com/firebasejs/7.0.0/firebase-app.js"></script>
    <script src="https://www.gstatic.com/firebasejs/7.0.0/firebase-auth.js"></script>
    <script src="https://www.gstatic.com/firebasejs/7.0.0/firebase-database.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src = "js/firebaseAuth.js"></script>
</head>
<body>
<div class="nav">
    <button onclick=userSignout()><a href="/">SignOut</a></button>
</div>

<div class="c1">
    <p>Questions</p>
    <input type="text" id = "ques" placeholder="Enter Question">
    <input type="text" id = "opt1" placeholder="Option 1">
    <input type="text" id = "opt2" placeholder="Option 2">
    <input type="text" id = "opt3" placeholder="Option 3">
    <input type="text" id = "opt4" placeholder="Option 4">
    <input type="text" id = "opt5" placeholder="Correct Option (enter number only)">
</div>

<div class="c2">
    <button id = "quess" onclick="add()">Add Question</button>
</div>

<div class="c1">
    <p>Enter Time</p>
    <input type="datetime-local" id ="qtime">
</div>

<div class="c2">
    <button id = "Create" onclick="createQuiz()">Create</button>
</div>

</body>
</html>